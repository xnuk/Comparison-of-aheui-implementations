doctype html
head
  meta(charset='utf-8')
  title Comparison of Aheui implementations
  link(rel='stylesheet', href='./index.css', type='text/css')
table#comp
  caption Comparison table
  tr
    th Snippets
    each _, i in data
      if i!='_'
        th
          a(href="https://github.com/"+i)=i
  tr
    td &nbsp;
  each _, i in data['aheui/caheui']
    if i!='_'
      tr
        th
          a(href="https://github.com/aheui/snippets/tree/master/"+i+".aheui")=i
        each v, j in data
          if j!='_'
            if v[i]===false
              td.fail
            else if v[i]===true
              td.terminated
            else
              td.pass=v[i].toString().replace(/(\.\d{3})\d+$/, '$1')+'s'
footer
  = "Last tested on "
  time(datetime=data._)=data._
